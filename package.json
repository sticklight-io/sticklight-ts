{
  "name": "@sticklight/sdk",
  "version": "0.1.9",
  "description": "The Official Sticklight TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "// compile": "// Also builds the dist package.",
    "compile": "pnpm run lint && tsup --minify terser",
    "test": "vitest watch --config tests/vitest.config.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "publish": "pnpm run compile && pnpm publish",
    "// ci:typecheck": "// For CI. 'pnpm run compile' already does typechecking (dts:true in tsup.config.ts)",
    "ci:typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "sticklight",
    "ai",
    "llm",
    "product analytics",
    "product",
    "analytics",
    "agent",
    "observability",
    "insights",
    "data-driven",
    "data",
    "platform",
    "openai",
    "anthropic",
    "gpt",
    "chatbot"
  ],
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.4",
    "@vitest/ui": "^3.0.6",
    "dotenv": "^16.4.7",
    "happy-dom": "^17.1.1",
    "husky": "^9.1.7",
    "terser": "^5.39.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "author": "Sticklight",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sticklight/sticklight-ts.git"
  },
  "bugs": {
    "url": "https://github.com/sticklight/sticklight-ts/issues"
  },
  "homepage": "https://sticklight.io"
}
