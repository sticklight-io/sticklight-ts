{
  "name": "@sticklight/sdk",
  "version": "0.1.1",
  "description": "The Official Sticklight TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "// compile": "// Also builds the dist package.",
    "compile": "tsup --minify terser",
    "dev": "tsup --watch",
    "test": "vitest watch --config tests/vitest.config.ts",
    "format": "biome format .",
    "lint": "biome lint .",
    "preversion": "npm run lint && npm test",
    "publish": "npm publish --access public",
    "// ci:typecheck": "// For CI. 'npm run dev' already does typechecking (dts:true in tsup.config.ts)",
    "ci:typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sticklight",
    "ai",
    "llm",
    "product analytics",
    "product",
    "analytics",
    "agent",
    "data",
    "platform",
    "openai",
    "anthropic",
    "gpt",
    "chatbot"
  ],
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^20.11.19",
    "@vitest/ui": "^3.0.5",
    "happy-dom": "^17.1.0",
    "terser": "^5.39.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  },
  "author": "Sticklight",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sticklight/sticklight-ts.git"
  },
  "bugs": {
    "url": "https://github.com/sticklight/sticklight-ts/issues"
  },
  "homepage": "https://sticklight.io"
}